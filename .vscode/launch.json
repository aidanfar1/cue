  "version": "0.2.0",
  "configurations": [
    {
      "name": "Cue Export",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}\\cmd\\cue",
      "args": ["export", "test.cue", "--out", "xml"]
    },
    {
      "name": "Cue Import",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}\\cmd\\cue",
      "args": ["import", "test.json", "-f"]
    },
    {
      "name": "Cue Import XML",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}\\cmd\\cue",
      "args": ["import", "test.xml"]
    },
    {
      "name": "Gen Go",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}\\internal\\filetypes\\gen.go"
    }
  ]
}
